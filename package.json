  "scripts": {
    "test:unit": "mocha ./test/unit/**/*.js",
    "test:functional": "mocha ./test/functional",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "serverless deploy",
    "remove": "serverless remove",
    "auth:log": "serverless logs -f auth",
