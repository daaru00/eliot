    "token:log": "serverless logs -f token",
    "token:log:watch": "serverless logs -f token -t",
    "authorizer:log": "serverless logs -f authorizer",
    "authorizer:log:watch": "serverless logs -f authorizer -t",
    "googleHome:log": "serverless logs -f googleHome",
    "googleHome:log:watch": "serverless logs -f googleHome -t",
    "googleHomeResync:log": "serverless logs -f googleHomeResync",
    "googleHomeResync:log:watch": "serverless logs -f googleHomeResync -t",
    "googleHomeResync:execute": "serverless invoke -f googleHomeResync",
    "googleHomeReport:log": "serverless logs -f googleHomeReport",
    "googleHomeReport:log:watch": "serverless logs -f googleHomeReport -t",
    "alexa:log": "serverless logs -f alexa",
    "alexa:log:watch": "serverless logs -f alexa -t",
    "alexaResync:log": "serverless logs -f alexaResync",
    "alexaResync:log:watch": "serverless logs -f alexaResync -t",
    "alexaResync:execute": "serverless invoke -f alexaResync",
    "alexaReport:log": "serverless logs -f alexaReport",
    "alexaReport:log:watch": "serverless logs -f alexaReport -t"
  }
